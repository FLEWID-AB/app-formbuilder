<part>
  <display-name>Form view</display-name>
  <form>
    <field-set name="form">
      <label>Form</label>
      <items>
        <input type="ContentSelector" name="form">
          <label>Form source (default: current content)</label>
          <occurrences minimum="0" maximum="1"/>
          <config>
            <relationship-type>system:reference</relationship-type>
            <allow-content-type>com.enonic.app.formbuilder:form</allow-content-type>
          </config>
        </input>
      </items>
    </field-set>
  </form>
</part>
